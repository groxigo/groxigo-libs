{
  "name": "@groxigo/icons",
  "version": "1.0.0",
  "description": "Tree-shakable Unicons icon components for React Native and Web",
  "main": "dist/index.js",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./dist/index.js",
      "types": "./src/index.ts"
    },
    "./line": {
      "import": "./src/line/index.ts",
      "types": "./src/line/index.ts"
    },
    "./solid": {
      "import": "./src/solid/index.ts",
      "types": "./src/solid/index.ts"
    },
    "./custom": {
      "import": "./src/custom/index.ts",
      "types": "./src/custom/index.ts"
    }
  },
  "scripts": {
    "generate": "bun run scripts/generate.ts",
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "icons",
    "unicons",
    "react-native",
    "react",
    "design-system"
  ],
  "author": "Groxigo",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/groxigo/groxigo-libs.git",
    "directory": "packages/icons"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "peerDependencies": {
    "react": "19.1.0",
    "react-native-svg": ">=15.0.0"
  },
  "peerDependenciesMeta": {
    "react-native-svg": {
      "optional": true
    }
  },
  "devDependencies": {
    "@iconscout/unicons": "^4.0.8",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.9",
    "@vitest/coverage-v8": "^3.0.0",
    "jsdom": "^25.0.0",
    "react-dom": "19.1.0",
    "react-native-svg": ">=15.0.0",
    "typescript": "~5.9.2",
    "vitest": "^3.0.0"
  },
  "files": [
    "src",
    "dist"
  ]
}
